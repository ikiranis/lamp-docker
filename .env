# Docker compose environment variables

# MySql credentials.
# Change them with every value you want
DATABASE_NAME=myweb
DATABASE_ROOT_PASSWORD=myweb

# Change the first part (7777) with every value you want
# The default url of the app is http://localhost:7777 or http://server_ip:7777
APP_PORT=7777:80

# Change the first part (3310) with every value you want
MYSQL_PORT=3320:3306